    "buildOptions": {
        "allowUnsafe": true,
        "optimize": true,
        "debugType": "portable",
        "platform": "x64",
        "copyToOutput": "dotnet-test-nunit"
    },
    "runtimes": {
        "win7-x64": {},
        "win8-x64": {},
        "win81-x64": {},
        "win10-x64": {},
        "osx.10.11-x64": {},
        "ubuntu.16.04-x64": {}
    }
