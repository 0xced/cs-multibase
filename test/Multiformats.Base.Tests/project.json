{
    "name": "Multiformats.Base.Tests",
    "version": "1.0.0-*",
    "testRunner": "nunit",
    "dependencies": {
        "NUnit": "3.6.0",
        "dotnet-test-nunit": "3.4.0-beta-3",
        "Multiformats.Base": {
            "target": "project"
        }
    },
    "frameworks": {
        "net461": {
            "dependencies": {
                "Microsoft.NETCore.Platforms": "1.1.0"
            }
        },
        "netcoreapp1.1": {
            "dependencies": {
                "Microsoft.NETCore.App": {
                    "version": "1.1.0",
                    "type": "platform"
                }
            }
        }
