{
    "name": "Multiformats.Base.Tests",
    "version": "1.0.0-*",
    "testRunner": "nunit",
    "dependencies": {
        "NUnit": "3.6.0",
        "dotnet-test-nunit": "3.4.0-beta-3",
        "Multiformats.Base": {
            "target": "project"
        }
    },
    "frameworks": {
        "net461": {
            "imports": [
                "dotnet5.6",
                "portable-net45+win8"
            ],
            "dependencies": {
                "Microsoft.NETCore.Platforms": "1.1.0"
            }
        },
        "netcoreapp1.0": {
            "dependencies": {
                "Microsoft.NETCore.App": {
                    "version": "1.0.1",
                    "type": "platform"
                }
            }
        }
    },
    "buildOptions": {
        "allowUnsafe": true,
        "optimize": true,
        "debugType": "portable",
        "platform": "x64",
        "copyToOutput": "dotnet-test-nunit"
    },
    "runtimes": {
        "win-x64": {},
        "osx-x64": {},
        "linux-x64": {}
    }
}